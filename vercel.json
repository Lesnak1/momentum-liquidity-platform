{
  "framework": "create-react-app",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/build",
  "functions": {
    "backend/vercel_app.py": "@vercel/python"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/vercel_app.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 